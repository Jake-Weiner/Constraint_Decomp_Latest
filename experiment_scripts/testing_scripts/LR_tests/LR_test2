#!/bin/bash

INPUT_DIR="/home/jake/PhD/Decomposition/Input/Unit_Testing/LR"
executable_path="/home/jake/PhD/Decomposition/Constraint_Decomp_Latest"
instance_name="LR_test1.mps"

if test -f $INPUT_FILE; then
    echo "$INPUT_FILE exists."
else
    echo "$INPUT_FILE does not exist" 
    exit 99
fi

$executable_path/main_test --input_root_folder=$INPUT_DIR \
--input_instance_name=$instance_name \
--run_LR_testing=true \
--general_decomp_file=/home/jake/PhD/Decomposition/Input/Unit_Testing/LR/test_convec_2.csv

#MAKE SURE THAT THE INITIAL DUAL VALUES ARE CHANGED

